{
  "name": "unleash/client",
  "description": "Unleash Client for PHP",
  "type": "library",
  "license": "Apache-2.0",
  "require": {
    "php": "^7.1.0",
    "ext-posix": "*",
    "ext-json": "*",
    "lastguest/murmurhash": "^2.0",
    "symfony/http-foundation": "^4.1",
    "symfony/event-dispatcher": "^4.1",
    "guzzlehttp/guzzle": "~6.0",
    "ext-pcntl": "*",
    "ext-ev": "*"
  },
  "autoload": {
    "files": [
      "src/Utils/normalizer.php"
    ],
    "psr-4": {
      "Unleash\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "7",
    "mockery/mockery": "^1.2",
    "friendsofphp/php-cs-fixer": "^2.13"
  }
}
